@using ISTC.FiveStage.AspNetCore.Views.Views_02.Models
@model ISTC.FiveStage.AspNetCore.Views.Views_02.Models.People

@{
    Layout = "_Layout";
    var contactViewModel = ViewData["ContactViewModel"] as ContactViewModel;
}

<br/>
<div>
    <ul style="background: azure">
        <p>@contactViewModel.FirstName</p> 
        <p>@contactViewModel.LastName</p> 
        <p><a href="@contactViewModel.Github" target="_blank">Github</a></p>
        <p><a href="@contactViewModel.Linkedin" target="_blank">Linkedin</a></p>
    </ul>
</div>
@await Html.PartialAsync("PartialHome",Model)
